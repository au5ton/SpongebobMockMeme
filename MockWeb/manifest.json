{
    "manifest_version": 2,

    "name": "MockWeb",
    "version": "1.1",

    "description": "now every website you go to will be a sponge bob mock meme",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "content_scripts" : [{
        "matches": [
            "<all_urls>"
        ],
        "js" : ["injected.js"]
    }]
}
